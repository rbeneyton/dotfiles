default partial
xkb_types "rb" {
    // declare our new type
    type "RB_BASE_SHIFT_CONTROL" {
        modifiers = Shift+Control;
        map[Shift] = Level2;
        map[Control] = Level3;
        level_name[Level1] = "Base";
        level_name[Level2] = "Shift";
        level_name[Level3] = "Control";
    };
};

